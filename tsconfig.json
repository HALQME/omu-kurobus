{
    "extends": "astro/tsconfigs/strict",
    "include": [".astro/types.d.ts", "**/*"],
    "exclude": ["dist"],
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@/src/*": ["src/*"],
            "@/actions/*": ["src/actions/*"],
            "@/components/*": ["src/components/*"],
            "@/layouts/*": ["src/layouts/*"],
            "@/styles/*": ["src/styles/*"],
            "@/pages/*": ["src/pages/*"],
            "@/utils/*": ["utils/*"],
            "@/types/*": ["types/*"]
        }
    }
}
